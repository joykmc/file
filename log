root@sonic:/home/admin# cat read.sh 
#!/bin/bash

for i in {1..128};
do
	ls /sys_switch/transceiver/eth$i/eeprom -l
	ls /sys_switch/transceiver/eth$i/tx_disable -l
	ls /sys_switch/transceiver/eth$i/rx_los -l
	ls /sys_switch/transceiver/eth$i/tx_fault -l
	echo "====================================="
done
